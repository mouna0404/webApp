import { OnInit } from '@angular/core';
import { Breadcrumb } from './breadcrumb.model';
import { MatBreadcrumbService } from './mat-breadcrumb.service';
import { ActivatedRoute, Router } from '@angular/router';
export declare class MatBreadcrumbComponent implements OnInit {
    private activatedRoute;
    private router;
    private ng7MatBreadcrumbService;
    breadcrumb: Breadcrumb[];
    bgColor: string;
    fontSize: string;
    fontColor: string;
    lastLinkColor: string;
    symbol: string;
    params: {
        [key: string]: any;
    };
    constructor(activatedRoute: ActivatedRoute, router: Router, ng7MatBreadcrumbService: MatBreadcrumbService);
    ngOnInit(): void;
    breadCrumbData(): void;
    private updateData;
}
